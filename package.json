{
  "name": "lightdm-webkit-girizgah",
  "version": "1.0.0",
  "description": "LightDM Webkit theme for Pardus Linux. Based on https://github.com/NoiSek/Aether",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/eslint src/es6/**",
    "build": "webpack --progress --colors --env.production",
    "build:langs": "npm run package:langs && npm run build && npm run package:langs",
    "package:langs": "NODE_ENV='production' babel-node src/scripts/mergeMessages.js",
    "build:dev": "webpack --progress --colors --env.development",
    "watch": "webpack --progress --colors --watch --env.development",
    "watch:dev": "webpack-dev-server --colors --env.development",
    "serve": "live-server --entry-file=index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/caglarturali/Girizgah.git"
  },
  "keywords": [
    "webkit",
    "aether",
    "girizgah",
    "lightdm",
    "login",
    "manager",
    "linux",
    "pardus"
  ],
  "author": "Caglar Turali",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/caglarturali/Girizgah/issues"
  },
  "homepage": "https://github.com/caglarturali/Girizgah#readme",
  "dependencies": {
    "babel-core": "~6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-react-intl": "^3.0.1",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "cryptiles": ">=4.1.2",
    "css-loader": "^0.28.11",
    "cxs": "^6.2.0",
    "eslint": "~4.19.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-jsx-control-statements": "^2.2.0",
    "eslint-plugin-react": "^7.4.0",
    "hoek": ">=4.2.1",
    "jquery": "^3.3.1",
    "jsx-control-statements": "^3.2.8",
    "lodash": ">=4.17.5",
    "macaddress": ">=0.2.9",
    "node-sass": "^4.9.0",
    "pixi.js": "^4.7.3",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-color": "^2.14.0",
    "react-dom": "^16.0.0",
    "react-intl": "^2.7.2",
    "react-redux": "^5.0.7",
    "reactcss": "^1.2.2",
    "redux": "~4.0.0",
    "sass-loader": "^7.0.1",
    "strftime": "^0.10.0",
    "style-loader": "^0.21.0",
    "svg-inline-loader": "^0.8.0",
    "tinycolor2": "^1.4.1",
    "webpack": "~4.8.2",
    "webpack-cli": "^2.1.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "live-server": "^1.2.1",
    "webpack-dev-server": "^3.1.10"
  }
}
